    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.17.0",
    "file-loader": "^0.8.4",
    "fsevents": "^0.3.8",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mocha": "^2.3.2",
    "normalize-css": "^2.3.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.0",
    "redux-logger": "^1.0.6",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  }
}
