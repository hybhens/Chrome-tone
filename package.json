{
  "name": "lr",
  "version": "0.1.0",
  "description": "line rider ported to the web browser",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --recursive --compilers js:babel/register",
    "serve": "node webpack-dev-server.js",
    "serve-prod": "NODE_ENV=production npm run serve",
    "get-fonts": "curl -o fontfiles.zip 'https://google-webfonts-helper.herokuapp.com/api/fonts/roboto?download=zip&subsets=latin&variants=regular,300,500&formats=woff,woff2' && unzip fontfiles.zip -d assets/fonts && rm fontfiles.zip",
    "make-config": "cp default-config.json config.json",
    "make-deploy-script": "git clone git@github.com:X1011/git-directory-deploy.git && chmod +x git-directory-deploy/deploy.sh",
    "build": "webpack --config webpack.dist.config.js && cp -r assets/* dist/",
    "deploy-init": "git-directory-deploy/deploy.sh -s",
    "deploy": "git-directory-deploy/deploy.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/conundrumer/lr.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conundrumer/lr/issues"
  },
  "homepage": "https://github.com/conundrumer/lr#readme",
  "dependencies": {
    "amf": "tootallnate/node-amf",
    "buffer": "^3.4.3",
    "chroma-js": "^1.1.1",
    "classnames": "^2.1.3",
    "combokeys": "^2.4.3",
    "immutable": "^3.7.5",
    "intersection": "0.0.1",
    "line-intersect": "^1.0.0",
    "lodash": "^3.10.1",
    "lz-string": "^1.4.4",
    "material-ui": "conundrumer/material-ui#build",
    "material-ui-mdi": "^0.2.0",
    "mouse-event": "^1.0.5",
    "neume.js": "^0.8.0",
    "normalize.css": "^3.0.3",
    "onecolor": "^2.5.0",
    "pui-css-code": "^1.10.0",
    "ray-vs-circle": "^2.0.0",
    "react": "^0.13.3",
    "react-dropzone": "^1.2.4",
    "react-inlinesvg": "conundrumer/react-inlinesvg#build",
    "react-pure-render": "^1.0.2",
    "react-redux": "^2.1.1",
    "react-slider": "^0.5.1",
    "react-stats": "0.0.5",
    "react-tap-event-plugin": "^0.1.7",
    "redux": "^2.0.0",
    "redux-logger": "^1.0.6",
    "redux-thunk": "^0.1.0",
    "reselect": "0.0.2",
    "rx": "^3.1.2",
    "vector2d": "conundrumer/vec2d"
  },
